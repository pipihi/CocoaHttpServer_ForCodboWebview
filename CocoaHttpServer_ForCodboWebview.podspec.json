{
  "name": "CocoaHttpServer_ForCodboWebview",
  "version": "2.4.2",
  "license": "BSD",
  "summary": "Edit from CocoaHTTPServer2 only for iOS/CodboWebview.",
  "homepage": "https://github.com/huangjimmy/CocoaHTTPServer",
  "authors": {
    "Robbie Hanson": "cocoahttpserver@googlegroups.com"
  },
  "source": {
    "git": "https://github.com/pipihi/CocoaHttpServer_ForCodboWebview.git",
    "tag": "2.4.2"
  },
  "source_files": "{Core,Extensions}/**/*.{h,m}",
  "requires_arc": true,
  "platforms": {
    "ios": "5.0"
  },

